    "editor.insertSpaces": true,
    "files.autoGuessEncoding": true,
    "arduino.logLevel": "info",
    "explorer.confirmDelete": false,
    "editor.detectIndentation": false,
    "arduino.useArduinoCli": true,
    "files.associations": {
      "cstddef": "cpp",
      "array": "cpp",
      "deque": "cpp",
      "string": "cpp",
      "unordered_map": "cpp",
      "vector": "cpp",
      "string_view": "cpp",
      "initializer_list": "cpp",
      "ranges": "cpp",
      "main_config.h": "c"
    },
}
